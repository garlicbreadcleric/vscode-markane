{
  "name": "markane",
  "displayName": "Markane",
  "repository": {"url": "https://github.com/garlicbreadcleric/vscode-markane"},
  "description": "Arcane focus for Markdown spell-casting",
  "publisher": "garlicbreadcleric",
  "icon": "icon.png",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.71.0"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "Markane: Restart language server",
        "command": "markane.restart"
      },
      {
        "title": "Markane: Show language server log",
        "command": "markane.log"
      },
      {
        "title": "Markane: Re-index documents",
        "command": "markane.index"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "vscode-languageclient": "^8.0.2",
    "yaml": "^2.1.3"
  }
}
